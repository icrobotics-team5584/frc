<config format="1.0">
  <robot name="MUCK">
    <wheels track="21" diameter="4" />
    <sensor mode="SensorSum" />
  </robot>
  <controls>
    <pathlimit>120</pathlimit>
    <velocity>5</velocity>
    <itp>10</itp>
    <t1>400</t1>
    <t2>200</t2>
  </controls>
  <units>
    <time>seconds</time>
    <length>inches</length>
    <angle>degrees</angle>
  </units>
  <profiles>
    <profile name="InitialisationToTrench01">
      <path>
        <segment id="1" type="turn" direction="forwards" turn="left" angle="45" length="24"/>
        <segment id="2" type="straight" direction="forwards" length="48"/>
        <segment id="3" type="turn" direction="forwards" turn="right" angle="45" length="24"/>
        <segment id="4" type="straight" direction="forwards" length="120"/>
      </path>
    </profile>
    <profile name="TrenchToPowerCellZone01">
      <path>
        <segment id="1" type="straight" direction="backwards" length="120"/>
        <segment id="2" type="turn" direction="backwards" turn="right" angle="45" length="24"/>
        <segment id="3" type="straight" direction="backwards" length="48"/>
        <segment id="4" type="turn" direction="backwards" turn="left" angle="45" length="24"/>
        <segment id="5" type="straight" direction="backwards" length="120"/>
      </path>
    </profile>
    <profile name="MotionProfile">
      <path>
        <segment id="1" type="turn" direction="forwards" turn="left" angle="90" length="96"/>
      </path>
    </profile>
  </profiles>
</config>
